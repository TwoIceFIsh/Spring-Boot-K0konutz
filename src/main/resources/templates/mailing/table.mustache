<table class="table">
    <thead>
    <tr>
        <th scope="col">No</th>
        <th scope="col">제목</th>
        <th scope="col">URL</th>
        <th scope="col">일자</th>
    </tr>
    </thead>
    <tbody>

    {{#article}}
        <tr>
            <th scope="row">{{no}}</th>
            <td>{{title}}</td>
            <td>{{url}}</td>
            <td>{{date}}</td>
        </tr>
    {{/article}}

    </tbody>
</table>