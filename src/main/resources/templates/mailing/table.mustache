<table class="table">
    <thead>
    <tr>
        <th scope="col">No</th>
        <th scope="col">제목</th>
        <th scope="col">URL</th>
        <th scope="col">일자</th>
    </tr>
    </thead>
    <tbody>

    {{#articles}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{title}}</td>
            <td><a class="text-black" href="{{url}}">{{url}}</a></td>
            <td>{{date}}</td>
        </tr>
    {{/articles}}

    </tbody>
</table>